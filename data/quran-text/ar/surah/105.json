{
  "id": 105,
  "nom": {
    "ar": "الفيل",
    "phonetique": "al-Fil",
    "fr": "",
    "en": ""
  },
  "type": "Makkiyah",
  "versets": [
    {
      "numero": 1,
      "texte_ar": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَٰبِ ٱلْفِيلِ"
    },
    {
      "numero": 2,
      "texte_ar": "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِى تَضْلِيلٍۢ"
    },
    {
      "numero": 3,
      "texte_ar": "وَأَرْسَلَ عَلَيْهِمْ طَيْرًا أَبَابِيلَ"
    },
    {
      "numero": 4,
      "texte_ar": "تَرْمِيهِم بِحِجَارَةٍۢ مِّن سِجِّيلٍۢ"
    },
    {
      "numero": 5,
      "texte_ar": "فَجَعَلَهُمْ كَعَصْفٍۢ مَّأْكُولٍۭ"
    }
  ]
}