{
  "id": 86,
  "nom": {
    "ar": "الطارق",
    "phonetique": "At-Tariq",
    "fr": "",
    "en": ""
  },
  "type": "Makkiyah",
  "versets": [
    {
      "numero": 1,
      "texte_ar": "وَٱلسَّمَآءِ وَٱلطَّارِقِ"
    },
    {
      "numero": 2,
      "texte_ar": "وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ"
    },
    {
      "numero": 3,
      "texte_ar": "ٱلنَّجْمُ ٱلثَّاقِبُ"
    },
    {
      "numero": 4,
      "texte_ar": "إِن كُلُّ نَفْسٍۢ لَّمَّا عَلَيْهَا حَافِظٌۭ"
    },
    {
      "numero": 5,
      "texte_ar": "فَلْيَنظُرِ ٱلْإِنسَٰنُ مِمَّ خُلِقَ"
    },
    {
      "numero": 6,
      "texte_ar": "خُلِقَ مِن مَّآءٍۢ دَافِقٍۢ"
    },
    {
      "numero": 7,
      "texte_ar": "يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ"
    },
    {
      "numero": 8,
      "texte_ar": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌۭ"
    },
    {
      "numero": 9,
      "texte_ar": "يَوْمَ تُبْلَى ٱلسَّرَآئِرُ"
    },
    {
      "numero": 10,
      "texte_ar": "فَمَا لَهُۥ مِن قُوَّةٍۢ وَلَا نَاصِرٍۢ"
    },
    {
      "numero": 11,
      "texte_ar": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ"
    },
    {
      "numero": 12,
      "texte_ar": "وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ"
    },
    {
      "numero": 13,
      "texte_ar": "إِنَّهُۥ لَقَوْلٌۭ فَصْلٌۭ"
    },
    {
      "numero": 14,
      "texte_ar": "وَمَا هُوَ بِٱلْهَزْلِ"
    },
    {
      "numero": 15,
      "texte_ar": "إِنَّهُمْ يَكِيدُونَ كَيْدًۭا"
    },
    {
      "numero": 16,
      "texte_ar": "وَأَكِيدُ كَيْدًۭا"
    },
    {
      "numero": 17,
      "texte_ar": "فَمَهِّلِ ٱلْكَٰفِرِينَ أَمْهِلْهُمْ رُوَيْدًۢا"
    }
  ]
}