    {
      "numero": 183,
      "texte_ar": "وَلَا تَبْخَسُوا۟ ٱلنَّاسَ أَشْيَآءَهُمْ وَلَا تَعْثَوْا۟ فِى ٱلْأَرْضِ مُفْسِدِينَ"
    },
    {
      "numero": 184,
      "texte_ar": "وَٱتَّقُوا۟ ٱلَّذِى خَلَقَكُمْ وَٱلْجِبِلَّةَ ٱلْأَوَّلِينَ"
    },
    {
      "numero": 185,
      "texte_ar": "قَالُوٓا۟ إِنَّمَآ أَنتَ مِنَ ٱلْمُسَحَّرِينَ"
    },
    {
      "numero": 186,
      "texte_ar": "وَمَآ أَنتَ إِلَّا بَشَرٌۭ مِّثْلُنَا وَإِن نَّظُنُّكَ لَمِنَ ٱلْكَٰذِبِينَ"
    },
    {
      "numero": 187,
      "texte_ar": "فَأَسْقِطْ عَلَيْنَا كِسَفًۭا مِّنَ ٱلسَّمَآءِ إِن كُنتَ مِنَ ٱلصَّٰدِقِينَ"
    },
    {
      "numero": 188,
      "texte_ar": "قَالَ رَبِّىٓ أَعْلَمُ بِمَا تَعْمَلُونَ"
    },
    {
      "numero": 189,
      "texte_ar": "فَكَذَّبُوهُ فَأَخَذَهُمْ عَذَابُ يَوْمِ ٱلظُّلَّةِ ۚ إِنَّهُۥ كَانَ عَذَابَ يَوْمٍ عَظِيمٍ"
    },
    {
      "numero": 190,
      "texte_ar": "إِنَّ فِى ذَٰلِكَ لَءَايَةًۭ ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
    },
    {
      "numero": 191,
      "texte_ar": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ"
    },
    {
      "numero": 192,
      "texte_ar": "وَإِنَّهُۥ لَتَنزِيلُ رَبِّ ٱلْعَٰلَمِينَ"
    },
    {
      "numero": 193,
      "texte_ar": "نَزَلَ بِهِ ٱلرُّوحُ ٱلْأَمِينُ"
    },
    {
      "numero": 194,
      "texte_ar": "عَلَىٰ قَلْبِكَ لِتَكُونَ مِنَ ٱلْمُنذِرِينَ"
    },
    {
      "numero": 195,
      "texte_ar": "بِلِسَانٍ عَرَبِىٍّۢ مُّبِينٍۢ"
    },
    {
      "numero": 196,
      "texte_ar": "وَإِنَّهُۥ لَفِى زُبُرِ ٱلْأَوَّلِينَ"
    },
    {
      "numero": 197,
      "texte_ar": "أَوَلَمْ يَكُن لَّهُمْ ءَايَةً أَن يَعْلَمَهُۥ عُلَمَٰٓؤُا۟ بَنِىٓ إِسْرَٰٓءِيلَ"
    },
    {
      "numero": 198,
      "texte_ar": "وَلَوْ نَزَّلْنَٰهُ عَلَىٰ بَعْضِ ٱلْأَعْجَمِينَ"
    },
    {
      "numero": 199,
      "texte_ar": "فَقَرَأَهُۥ عَلَيْهِم مَّا كَانُوا۟ بِهِۦ مُؤْمِنِينَ"
    },
    {
      "numero": 200,
      "texte_ar": "كَذَٰلِكَ سَلَكْنَٰهُ فِى قُلُوبِ ٱلْمُجْرِمِينَ"
    },
    {
      "numero": 201,
      "texte_ar": "لَا يُؤْمِنُونَ بِهِۦ حَتَّىٰ يَرَوُا۟ ٱلْعَذَابَ ٱلْأَلِيمَ"
    },
    {
      "numero": 202,
      "texte_ar": "فَيَأْتِيَهُم بَغْتَةًۭ وَهُمْ لَا يَشْعُرُونَ"
    },
    {
      "numero": 203,
      "texte_ar": "فَيَقُولُوا۟ هَلْ نَحْنُ مُنظَرُونَ"
    },
    {
      "numero": 204,
      "texte_ar": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ"
    },
    {
      "numero": 205,
      "texte_ar": "أَفَرَءَيْتَ إِن مَّتَّعْنَٰهُمْ سِنِينَ"
    },
    {
      "numero": 206,
      "texte_ar": "ثُمَّ جَآءَهُم مَّا كَانُوا۟ يُوعَدُونَ"
    },
    {
      "numero": 207,
      "texte_ar": "مَآ أَغْنَىٰ عَنْهُم مَّا كَانُوا۟ يُمَتَّعُونَ"
    },
    {
      "numero": 208,
      "texte_ar": "وَمَآ أَهْلَكْنَا مِن قَرْيَةٍ إِلَّا لَهَا مُنذِرُونَ"
    },
    {
      "numero": 209,
      "texte_ar": "ذِكْرَىٰ وَمَا كُنَّا ظَٰلِمِينَ"
    },
    {
      "numero": 210,
      "texte_ar": "وَمَا تَنَزَّلَتْ بِهِ ٱلشَّيَٰطِينُ"
    },
    {
      "numero": 211,
      "texte_ar": "وَمَا يَنۢبَغِى لَهُمْ وَمَا يَسْتَطِيعُونَ"
    },
    {
      "numero": 212,
      "texte_ar": "إِنَّهُمْ عَنِ ٱلسَّمْعِ لَمَعْزُولُونَ"
    },
    {
      "numero": 213,
      "texte_ar": "فَلَا تَدْعُ مَعَ ٱللَّهِ إِلَٰهًا ءَاخَرَ فَتَكُونَ مِنَ ٱلْمُعَذَّبِينَ"
    },
    {
      "numero": 214,
      "texte_ar": "وَأَنذِرْ عَشِيرَتَكَ ٱلْأَقْرَبِينَ"
    },
    {
      "numero": 215,
      "texte_ar": "وَٱخْفِضْ جَنَاحَكَ لِمَنِ ٱتَّبَعَكَ مِنَ ٱلْمُؤْمِنِينَ"
    },
    {
      "numero": 216,
      "texte_ar": "فَإِنْ عَصَوْكَ فَقُلْ إِنِّى بَرِىٓءٌۭ مِّمَّا تَعْمَلُونَ"
    },
    {
      "numero": 217,
      "texte_ar": "وَتَوَكَّلْ عَلَى ٱلْعَزِيزِ ٱلرَّحِيمِ"
    },
    {
      "numero": 218,
      "texte_ar": "ٱلَّذِى يَرَىٰكَ حِينَ تَقُومُ"
    },
    {
      "numero": 219,
      "texte_ar": "وَتَقَلُّبَكَ فِى ٱلسَّٰجِدِينَ"
    },
    {
      "numero": 220,
      "texte_ar": "إِنَّهُۥ هُوَ ٱلسَّمِيعُ ٱلْعَلِيمُ"
    },
    {
      "numero": 221,
      "texte_ar": "هَلْ أُنَبِّئُكُمْ عَلَىٰ مَن تَنَزَّلُ ٱلشَّيَٰطِينُ"
    },
    {
      "numero": 222,
      "texte_ar": "تَنَزَّلُ عَلَىٰ كُلِّ أَفَّاكٍ أَثِيمٍۢ"
    },
    {
      "numero": 223,
      "texte_ar": "يُلْقُونَ ٱلسَّمْعَ وَأَكْثَرُهُمْ كَٰذِبُونَ"
    },
    {
      "numero": 224,
      "texte_ar": "وَٱلشُّعَرَآءُ يَتَّبِعُهُمُ ٱلْغَاوُۥنَ"
    },
    {
      "numero": 225,
      "texte_ar": "أَلَمْ تَرَ أَنَّهُمْ فِى كُلِّ وَادٍۢ يَهِيمُونَ"
    },
    {
      "numero": 226,
      "texte_ar": "وَأَنَّهُمْ يَقُولُونَ مَا لَا يَفْعَلُونَ"
    },
    {
      "numero": 227,
      "texte_ar": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ وَذَكَرُوا۟ ٱللَّهَ كَثِيرًۭا وَٱنتَصَرُوا۟ مِنۢ بَعْدِ مَا ظُلِمُوا۟ ۗ وَسَيَعْلَمُ ٱلَّذِينَ ظَلَمُوٓا۟ أَىَّ مُنقَلَبٍۢ يَنقَلِبُونَ"
    }
  ]
}