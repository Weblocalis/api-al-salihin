{
  "id": 95,
  "nom": {
    "ar": "التين",
    "phonetique": "at-Tin",
    "fr": "",
    "en": ""
  },
  "type": "Makkiyah",
  "versets": [
    {
      "numero": 1,
      "texte_ar": "وَٱلتِّينِ وَٱلزَّيْتُونِ"
    },
    {
      "numero": 2,
      "texte_ar": "وَطُورِ سِينِينَ"
    },
    {
      "numero": 3,
      "texte_ar": "وَهَٰذَا ٱلْبَلَدِ ٱلْأَمِينِ"
    },
    {
      "numero": 4,
      "texte_ar": "لَقَدْ خَلَقْنَا ٱلْإِنسَٰنَ فِىٓ أَحْسَنِ تَقْوِيمٍۢ"
    },
    {
      "numero": 5,
      "texte_ar": "ثُمَّ رَدَدْنَٰهُ أَسْفَلَ سَٰفِلِينَ"
    },
    {
      "numero": 6,
      "texte_ar": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ فَلَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍۢ"
    },
    {
      "numero": 7,
      "texte_ar": "فَمَا يُكَذِّبُكَ بَعْدُ بِٱلدِّينِ"
    },
    {
      "numero": 8,
      "texte_ar": "أَلَيْسَ ٱللَّهُ بِأَحْكَمِ ٱلْحَٰكِمِينَ"
    }
  ]
}