  },
  "type": "Makkiyah",
  "versets": [
    {
      "numero": 1,
      "texte_ar": "قُلْ أَعُوذُ بِرَبِّ ٱلْفَلَقِ"
    },
    {
      "numero": 2,
      "texte_ar": "مِن شَرِّ مَا خَلَقَ"
    },
    {
      "numero": 3,
      "texte_ar": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ"
    },
    {
      "numero": 4,
      "texte_ar": "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِى ٱلْعُقَدِ"
    },
    {
      "numero": 5,
      "texte_ar": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ"
    }
  ]
}