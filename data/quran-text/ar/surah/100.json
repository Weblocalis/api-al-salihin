{
  "id": 100,
  "nom": {
    "ar": "العاديات",
    "phonetique": "al-'Adiyat ",
    "fr": "",
    "en": ""
  },
  "type": "Makkiyah",
  "versets": [
    {
      "numero": 1,
      "texte_ar": "وَٱلْعَٰدِيَٰتِ ضَبْحًۭا"
    },
    {
      "numero": 2,
      "texte_ar": "فَٱلْمُورِيَٰتِ قَدْحًۭا"
    },
    {
      "numero": 3,
      "texte_ar": "فَٱلْمُغِيرَٰتِ صُبْحًۭا"
    },
    {
      "numero": 4,
      "texte_ar": "فَأَثَرْنَ بِهِۦ نَقْعًۭا"
    },
    {
      "numero": 5,
      "texte_ar": "فَوَسَطْنَ بِهِۦ جَمْعًا"
    },
    {
      "numero": 6,
      "texte_ar": "إِنَّ ٱلْإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٌۭ"
    },
    {
      "numero": 7,
      "texte_ar": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٌۭ"
    },
    {
      "numero": 8,
      "texte_ar": "وَإِنَّهُۥ لِحُبِّ ٱلْخَيْرِ لَشَدِيدٌ"
    },
    {
      "numero": 9,
      "texte_ar": "۞ أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِى ٱلْقُبُورِ"
    },
    {
      "numero": 10,
      "texte_ar": "وَحُصِّلَ مَا فِى ٱلصُّدُورِ"
    },
    {
      "numero": 11,
      "texte_ar": "إِنَّ رَبَّهُم بِهِمْ يَوْمَئِذٍۢ لَّخَبِيرٌۢ"
    }
  ]
}