{
  "id": 90,
  "nom": {
    "ar": "البلد",
    "phonetique": "al-Balad",
    "fr": "",
    "en": ""
  },
  "type": "Makkiyah",
  "versets": [
    {
      "numero": 1,
      "texte_ar": "لَآ أُقْسِمُ بِهَٰذَا ٱلْبَلَدِ"
    },
    {
      "numero": 2,
      "texte_ar": "وَأَنتَ حِلٌّۢ بِهَٰذَا ٱلْبَلَدِ"
    },
    {
      "numero": 3,
      "texte_ar": "وَوَالِدٍۢ وَمَا وَلَدَ"
    },
    {
      "numero": 4,
      "texte_ar": "لَقَدْ خَلَقْنَا ٱلْإِنسَٰنَ فِى كَبَدٍ"
    },
    {
      "numero": 5,
      "texte_ar": "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌۭ"
    },
    {
      "numero": 6,
      "texte_ar": "يَقُولُ أَهْلَكْتُ مَالًۭا لُّبَدًا"
    },
    {
      "numero": 7,
      "texte_ar": "أَيَحْسَبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌ"
    },
    {
      "numero": 8,
      "texte_ar": "أَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ"
    },
    {
      "numero": 9,
      "texte_ar": "وَلِسَانًۭا وَشَفَتَيْنِ"
    },
    {
      "numero": 10,
      "texte_ar": "وَهَدَيْنَٰهُ ٱلنَّجْدَيْنِ"
    },
    {
      "numero": 11,
      "texte_ar": "فَلَا ٱقْتَحَمَ ٱلْعَقَبَةَ"
    },
    {
      "numero": 12,
      "texte_ar": "وَمَآ أَدْرَىٰكَ مَا ٱلْعَقَبَةُ"
    },
    {
      "numero": 13,
      "texte_ar": "فَكُّ رَقَبَةٍ"
    },
    {
      "numero": 14,
      "texte_ar": "أَوْ إِطْعَٰمٌۭ فِى يَوْمٍۢ ذِى مَسْغَبَةٍۢ"
    },
    {
      "numero": 15,
      "texte_ar": "يَتِيمًۭا ذَا مَقْرَبَةٍ"
    },
    {
      "numero": 16,
      "texte_ar": "أَوْ مِسْكِينًۭا ذَا مَتْرَبَةٍۢ"
    },
    {
      "numero": 17,
      "texte_ar": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُوا۟ وَتَوَاصَوْا۟ بِٱلصَّبْرِ وَتَوَاصَوْا۟ بِٱلْمَرْحَمَةِ"
    },
    {
      "numero": 18,
      "texte_ar": "أُو۟لَٰٓئِكَ أَصْحَٰبُ ٱلْمَيْمَنَةِ"
    },
    {
      "numero": 19,
      "texte_ar": "وَٱلَّذِينَ كَفَرُوا۟ بِـَٔايَٰتِنَا هُمْ أَصْحَٰبُ ٱلْمَشْـَٔمَةِ"
    },
    {
      "numero": 20,
      "texte_ar": "عَلَيْهِمْ نَارٌۭ مُّؤْصَدَةٌۢ"
    }
  ]
}