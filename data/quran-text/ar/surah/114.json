  "type": "Makkiyah",
  "versets": [
    {
      "numero": 1,
      "texte_ar": "قُلْ أَعُوذُ بِرَبِّ ٱلنَّاسِ"
    },
    {
      "numero": 2,
      "texte_ar": "مَلِكِ ٱلنَّاسِ"
    },
    {
      "numero": 3,
      "texte_ar": "إِلَٰهِ ٱلنَّاسِ"
    },
    {
      "numero": 4,
      "texte_ar": "مِن شَرِّ ٱلْوَسْوَاسِ ٱلْخَنَّاسِ"
    },
    {
      "numero": 5,
      "texte_ar": "ٱلَّذِى يُوَسْوِسُ فِى صُدُورِ ٱلنَّاسِ"
    },
    {
      "numero": 6,
      "texte_ar": "مِنَ ٱلْجِنَّةِ وَٱلنَّاسِ"
    }
  ]
}