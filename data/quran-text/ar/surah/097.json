  "id": 97,
  "nom": {
    "ar": "القدر",
    "phonetique": "al-Qadr",
    "fr": "",
    "en": ""
  },
  "type": "Makkiyah",
  "versets": [
    {
      "numero": 1,
      "texte_ar": "إِنَّآ أَنزَلْنَٰهُ فِى لَيْلَةِ ٱلْقَدْرِ"
    },
    {
      "numero": 2,
      "texte_ar": "وَمَآ أَدْرَىٰكَ مَا لَيْلَةُ ٱلْقَدْرِ"
    },
    {
      "numero": 3,
      "texte_ar": "لَيْلَةُ ٱلْقَدْرِ خَيْرٌۭ مِّنْ أَلْفِ شَهْرٍۢ"
    },
    {
      "numero": 4,
      "texte_ar": "تَنَزَّلُ ٱلْمَلَٰٓئِكَةُ وَٱلرُّوحُ فِيهَا بِإِذْنِ رَبِّهِم مِّن كُلِّ أَمْرٍۢ"
    },
    {
      "numero": 5,
      "texte_ar": "سَلَٰمٌ هِىَ حَتَّىٰ مَطْلَعِ ٱلْفَجْرِ"
    }
  ]
}