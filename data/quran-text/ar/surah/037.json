    {
      "numero": 178,
      "texte_ar": "وَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍۢ"
    },
    {
      "numero": 179,
      "texte_ar": "وَأَبْصِرْ فَسَوْفَ يُبْصِرُونَ"
