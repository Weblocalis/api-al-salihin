      "texte_ar": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْءَاخِرِينَ"
    },
    {
      "numero": 109,
      "texte_ar": "سَلَٰمٌ عَلَىٰٓ إِبْرَٰهِيمَ"
    },
    {
      "numero": 110,
      "texte_ar": "كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ"
    },
    {
      "numero": 111,
      "texte_ar": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ"
    },
    {
      "numero": 112,
      "texte_ar": "وَبَشَّرْنَٰهُ بِإِسْحَٰقَ نَبِيًّۭا مِّنَ ٱلصَّٰلِحِينَ"
    },
    {
      "numero": 113,
      "texte_ar": "وَبَٰرَكْنَا عَلَيْهِ وَعَلَىٰٓ إِسْحَٰقَ ۚ وَمِن ذُرِّيَّتِهِمَا مُحْسِنٌۭ وَظَالِمٌۭ لِّنَفْسِهِۦ مُبِينٌۭ"
    },
    {
      "numero": 114,
      "texte_ar": "وَلَقَدْ مَنَنَّا عَلَىٰ مُوسَىٰ وَهَٰرُونَ"
    },
    {
      "numero": 115,
      "texte_ar": "وَنَجَّيْنَٰهُمَا وَقَوْمَهُمَا مِنَ ٱلْكَرْبِ ٱلْعَظِيمِ"
    },
    {
      "numero": 116,
      "texte_ar": "وَنَصَرْنَٰهُمْ فَكَانُوا۟ هُمُ ٱلْغَٰلِبِينَ"
    },
    {
      "numero": 117,
      "texte_ar": "وَءَاتَيْنَٰهُمَا ٱلْكِتَٰبَ ٱلْمُسْتَبِينَ"
    },
    {
      "numero": 118,
      "texte_ar": "وَهَدَيْنَٰهُمَا ٱلصِّرَٰطَ ٱلْمُسْتَقِيمَ"
    },
    {
      "numero": 119,
      "texte_ar": "وَتَرَكْنَا عَلَيْهِمَا فِى ٱلْءَاخِرِينَ"
    },
    {
      "numero": 120,
      "texte_ar": "سَلَٰمٌ عَلَىٰ مُوسَىٰ وَهَٰرُونَ"
    },
    {
      "numero": 121,
      "texte_ar": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ"
