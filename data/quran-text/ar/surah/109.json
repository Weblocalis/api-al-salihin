  "type": "Makkiyah",
  "versets": [
    {
      "numero": 1,
      "texte_ar": "قُلْ يَٰٓأَيُّهَا ٱلْكَٰفِرُونَ"
    },
    {
      "numero": 2,
      "texte_ar": "لَآ أَعْبُدُ مَا تَعْبُدُونَ"
    },
    {
      "numero": 3,
      "texte_ar": "وَلَآ أَنتُمْ عَٰبِدُونَ مَآ أَعْبُدُ"
    },
    {
      "numero": 4,
      "texte_ar": "وَلَآ أَنَا۠ عَابِدٌۭ مَّا عَبَدتُّمْ"
    },
    {
      "numero": 5,
      "texte_ar": "وَلَآ أَنتُمْ عَٰبِدُونَ مَآ أَعْبُدُ"
    },
    {
      "numero": 6,
      "texte_ar": "لَكُمْ دِينُكُمْ وَلِىَ دِينِ"
    }
  ]
}