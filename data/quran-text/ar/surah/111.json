  "nom": {
    "ar": "المسد",
    "phonetique": "Al-Masad",
    "fr": "",
    "en": ""
  },
  "type": "Makkiyah",
  "versets": [
    {
      "numero": 1,
      "texte_ar": "تَبَّتْ يَدَآ أَبِى لَهَبٍۢ وَتَبَّ"
    },
    {
      "numero": 2,
      "texte_ar": "مَآ أَغْنَىٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَ"
    },
    {
      "numero": 3,
      "texte_ar": "سَيَصْلَىٰ نَارًۭا ذَاتَ لَهَبٍۢ"
    },
    {
      "numero": 4,
      "texte_ar": "وَٱمْرَأَتُهُۥ حَمَّالَةَ ٱلْحَطَبِ"
    },
    {
      "numero": 5,
      "texte_ar": "فِى جِيدِهَا حَبْلٌۭ مِّن مَّسَدٍۭ"
    }
  ]
}